			$('#enlargedViewContainer').css('width','');
				
			$('#enlargedViewContent').text('').append(html);

			if(fadeInImages == '1')
			{
				bindImageFadeIn('#enlargedViewContent img');	
			}

			cycleStandardPage(pagebelow);

			bindEnlargedViewControls();
			
