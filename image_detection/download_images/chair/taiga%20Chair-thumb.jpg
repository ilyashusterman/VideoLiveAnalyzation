  <script>
	$(document).ready(function(){
		var lang,
			lastLang='',
			langRel={'ru': 'rus', 'en': 'eng', 'uk': 'ukr'};
	    if (navigator
	            && navigator.userAgent
	            && (lang = navigator.userAgent
	                    .match(/android.*\W(\w\w)-(\w\w)\W/i))) {
	        lang = lang[1];
	    }

	    if (!lang && navigator) {
	        if (navigator.language) {
	            lang = navigator.language;
	        } else if (navigator.browserLanguage) {
	            lang = navigator.browserLanguage;
	        } else if (navigator.systemLanguage) {
	            lang = navigator.systemLanguage;
	        } else if (navigator.userLanguage) {
	            lang = navigator.userLanguage;
	        }
	        lang = lang.substr(0, 2);
	    }
	    // переводим название языка в соответствие с основным сайтом
	    lang=langRel[lang];

	    function showLang(lng) {
	    	$('.lang').hide();
			$('.lang.'+lng).slideDown(300);
			resetBanners(lng);
			lastLang=lng;
	    }
	    showLang(lang)

	    function makeBanners() {
	    	var lng,  pbpL, bnContainer,
	    		p, target, t;
	    	for (lng in pbp) {
	    		resetBanners(lng);
	    	}
	    }
	    function resetBanners(lng) {
	    	var pbpL=pbp[lng],
				bnContainer=$('.lang.'+lng).find('.id-banners');
		    //console.log(lng);
    		if (bnContainer.size() && pbpL.length) {
    			var p=new Array(),
    				t='', target, i;
    			for (i=0;i<pbpL.length;i++) { p[i] = pbpL[i]; }
    			for (i=0;i<2;i++) {
    				var idx = Math.round(100000*Math.random()) % p.length;						
				    if (p[idx].newwin) { target = ' target="_blank" '; } else { target = ''; }
					t = t + '<li class="preview-box-item"><div><a '+target+'class="preview-image '+p[idx].c+'" href="'+p[idx].l+'" >' +
							'<img src="http://nic.ua/f/i/backgrounds/transparent.gif" alt=""/>' +
							'<span class="title"><span>'+p[idx].t+'</span></span>' +
		                    ((p[idx].r)?('<span class="notice"><span>'+p[idx].r+'</span></span>'):'')+
							'</a><p>'+p[idx].n+'</p></div></li>';								
					p.splice(idx,1);
    			}
    			bnContainer.html('<ul class="preview-box-list nm">'+t+'</ul>');
    		}
	    }
	    makeBanners();
		
		$('.show-lang').click(function(){
			var lng=$(this).attr('href').substr(1);
			showLang(lng);
		});


		var lastHash=window.location.hash.substr(1);
		function checkHash() {
			var curHash=window.location.hash.substr(1);
			if (curHash!=lastHash) {
				var lng=curHash;
				if (lastLang!=lng) {
					switch (lng) {
					    case 'rus':
					    case 'eng':
					    case 'ukr': {
					    	showLang(lng);
							break;
					    }
					}
				}
				lastHash=curHash;
			}
			setTimeout(checkHash,100);
		}
		setTimeout(checkHash,100);

		
	});
